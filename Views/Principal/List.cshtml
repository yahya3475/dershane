@model List<dershane.Models.User>

<div class="container mt-4">
    <h2>User List</h2>

    <table class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>School Number</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Role</th>
            </tr>
        </thead>
        <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.dershaneid</td>
                <td>@user.firstname</td>
                <td>@user.lastname</td>
                <td>@user.role</td>
            </tr>
        }
        </tbody>
    </table>
</div>
